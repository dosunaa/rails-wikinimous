<div class="container">
  <h1>Articles</h1>
    <% @articles.each do |article| %>
      <h3><%= link_to article.title, article %> | <%= link_to 'edit', edit_article_path(article)%> | <%= link_to 'delete', article, method: "delete",  data: { confirm: "Delete #{article.title}?" }%></h3>
      <p><%= article.content%></p>
    <% end %>
</div>
